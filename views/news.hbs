<script type="text/javascript">
    /* create namespace maker */
    var createNamespace;
    if (!createNamespace || typeof(createNamespace) != "function") {
        createNamespace = function(namespace) {
            var names = namespace.split(".");
            var w = window;
            for(var i=0; i<names.length; i++) {
                if(!w[names[i]]) {
                    w[names[i]] = {};
                }
                w = w[names[i]];
            }
        };
    };

    createNamespace("ninemsn");

    /**
     * [arguments:7] = 0:catId, 1:inputText, 2:inputId, 3:buttonId, 4:opacity, 5:descLength, 6:isDebug
     */
    ninemsn.shoppingModule = function() {
        /* private variables */
        SHOPPING_URL_BASE = "http://shopping.ninemsn.com.au/results/shp/?scId=1&text=";
        TITLE_DELIMETER = " - $";

        /* construct instance */
        this.create = function(args) {
            this.catId = args[0];
            this.inputText = args[1] || "Search Shopping";
            this.inputId = args[2] || "shopping_module_search";
            this.buttonId = args[3] || "shopping_module_go";
            this.opacity = args[4] || 0.6;
            this.descLength = args[5] || 45;
            this.isDebug = args[6];
        };

        /* execute the required functionalities */
        this.execute = function() {
            try {
                this.isDebug = this.getParamValue("isDebug") == "true" ? true : this.isDebug;
                this.input = document.getElementById(this.inputId);

                var catPanel = document.getElementById(this.catId)
                if(catPanel) {
                    this.initialiseModule(catPanel);
                }
                else if(this.isDebug) {
                    alert("[ninemsn.shoppingModule.execute] No category panel for category ID:" + this.catId);
                };

                if(this.input) {
                    this.addInputText();
                }
                else if(this.isDebug) {
                    alert("[ninemsn.shoppingModule.execute] No input tag for input ID:" + this.inputId);
                };
            } catch(e) {
                if(this.isDebug) alert("[ninemsn.shoppingModule.execute] " + e);
            };
        };

        /* change the opacity depending on mouse */
        this.changeOpacity = function(isOver) {
            try {
                this.setOpacity(this.buttonId, (isOver ? this.opacity : 1));
            } catch(e) {
                if(this.isDebug) alert("[ninemsn.shoppingModule.changeOpacity] " + e);
            };
        };

        /* get the keyword and redirect to the shopping site */
        this.submit = function() {
            var keyword = "";
            if(this.input) {
                keyword = this.input.value;

                if(keyword == this.inputText) {
                    this.input.value = "";
                    keyword = "";
                };
            };
            window.location = SHOPPING_URL_BASE + keyword;
        };

        /* initialise the images and the texts */
        this.initialiseModule = function(catPanel) {
            var links = catPanel.childNodes;
            for(var i=0; i<links.length; i++) {
                if((links[i].className == "odd") || (links[i].className == "even")) {
                    var products = links[i].childNodes;
                    for(var j=0; j<products.length; j++) {
                        if(products[j].className == "image") {
                            this.handleImages(products[j].childNodes);
                        }
                        else if(products[j].className == "title") {
                            this.handleTitles(products[j].childNodes);
                        };
                    };
                };
            };
        };

        /* attach the opacity event to the images  */
        this.handleImages = function(imageNodes) {
            for(var i=0; i<imageNodes.length; i++) {
                if(imageNodes[i].tagName && (imageNodes[i].tagName.toLowerCase() == "a")) {
                    var images = imageNodes[i].childNodes;
                    for(var j=0; j<images.length; j++) {
                        if(images[j].tagName && (images[j].tagName.toLowerCase() == "img")) {
                            var self = this;
                            images[j].onmouseover = function() {self.setOpacity(this,self.opacity);};
                            images[j].onmouseout = function() {self.setOpacity(this,1);};
                        };
                    };
                };
            };
        };

        /* split & truncate the titles  */
        this.handleTitles = function(titleNodes) {
            for(var i=0; i<titleNodes.length; i++) {
                if(titleNodes[i].className && (titleNodes[i].className.indexOf("linkItem") > -1)) {
                    var title = titleNodes[i].innerHTML;
                    var pos = title.lastIndexOf(TITLE_DELIMETER);
                    if(pos > -1) {
                        var desc = title.substring(0,pos-1);
                        if(desc.length > this.descLength) {
                            desc = desc.substring(0, this.descLength) + " ...";
                        };
                        var price = title.substring(pos, title.length);

                        titleNodes[i].innerHTML = desc + price;
                    };
                };
            };
        };

        /* construct this instance */
        this.addInputText = function() {
            var self = this;

            /* Add event handlers */
            this.input.onfocus = function(e) {
                var target = window.event ? window.event.srcElement : e ? e.target : null;
                if(target && (target.value == self.inputText)) {
                    target.value = "";
                };
            };

            this.input.onblur = function(e) {
                var target = window.event ? window.event.srcElement : e ? e.target : null;
                if(target && (target.value == "" && self.inputText)) {
                    target.value = target.defaultText;
                };
            };

            /* Save the current value */
            if (this.input.value != "") {
                this.input.defaultText = this.input.value;
            }
            else if(this.inputText) {
                this.input.value = this.inputText;
                this.input.defaultText = this.inputText;
            };
        };

        /** return value for a param from a query */
        this.getParamValue = function(param, query) {
            query = query || window.location.href;
            var value;
            var startPos = query.indexOf(param + "=");
            if(startPos > -1) {
                startPos += param.length+1;
                var endPos = query.indexOf("&",startPos+1);
                if(endPos < 0) {
                    endPos = query.length;
                };
                value = query.substring(startPos,endPos);
            };

            return value;
        };

        /** change the opacity of the given element */
        this.setOpacity = function(element, opacity) {
            element = typeof(element) == "string" ? document.getElementById(element) : element;
            element.style.opacity = opacity;
            element.style.filter = "alpha(opacity=" + opacity*100 + ")";
        };

        this.create(arguments);
    };

    if (window.schmod_catid && window.schmod_schtext)
    {
        var shoppingModule = new ninemsn.shoppingModule(schmod_catid, schmod_schtext, "schmod_text", "schmod_submit");
        shoppingModule.execute();
    }
